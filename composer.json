{
    "name": "addgod/laravel-translation-manager",
    "description": "Manage Laravel Translations",
    "keywords": ["laravel", "translations", "translator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jesper Kold-Hansen",
            "email": "jkoldhansen@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8",
        "illuminate/support": "^5.5|^6|^7|^8|^9|^10",
        "illuminate/translation": "^5.5|^6|^7|^8|^9|^10",
        "symfony/finder": "^3|^4|^5|^6"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0",
        "phpunit/phpunit": "~7.0|^8.0|^9.0"
    },
    "scripts": {
        "test": "phpunit tests",
        "test-coverage": "phpdbg -qrr vendor/bin/phpunit --coverage-html tests/_reports"
    },
    "autoload": {
        "psr-4": {
            "Addgod\\TranslationManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Addgod\\TranslationManager\\Tests\\": "tests\\Unit"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addgod\\TranslationManager\\ManagerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
